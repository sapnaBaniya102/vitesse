#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/js-yaml@3.14.1/node_modules/js-yaml/bin/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/js-yaml@3.14.1/node_modules/js-yaml/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/js-yaml@3.14.1/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/node_modules:/Applications/XAMPP/xamppfiles/htdocs/node_modules:/Applications/XAMPP/xamppfiles/node_modules:/Applications/XAMPP/node_modules:/Applications/node_modules:/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/gray-matter@4.0.3/node_modules/gray-matter/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/gray-matter@4.0.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/js-yaml@3.14.1/node_modules/js-yaml/bin/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/js-yaml@3.14.1/node_modules/js-yaml/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/js-yaml@3.14.1/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/node_modules:/Applications/XAMPP/xamppfiles/htdocs/node_modules:/Applications/XAMPP/xamppfiles/node_modules:/Applications/XAMPP/node_modules:/Applications/node_modules:/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/gray-matter@4.0.3/node_modules/gray-matter/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/gray-matter@4.0.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../js-yaml@3.14.1/node_modules/js-yaml/bin/js-yaml.js" "$@"
else
  exec node  "$basedir/../../../../../js-yaml@3.14.1/node_modules/js-yaml/bin/js-yaml.js" "$@"
fi
