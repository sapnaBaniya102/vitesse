#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/regjsparser@0.7.0/node_modules/regjsparser/bin/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/regjsparser@0.7.0/node_modules/regjsparser/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/regjsparser@0.7.0/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/node_modules:/Applications/XAMPP/xamppfiles/htdocs/node_modules:/Applications/XAMPP/xamppfiles/node_modules:/Applications/XAMPP/node_modules:/Applications/node_modules:/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/regexpu-core@4.8.0/node_modules/regexpu-core/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/regexpu-core@4.8.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/regjsparser@0.7.0/node_modules/regjsparser/bin/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/regjsparser@0.7.0/node_modules/regjsparser/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/regjsparser@0.7.0/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/node_modules:/Applications/XAMPP/xamppfiles/htdocs/node_modules:/Applications/XAMPP/xamppfiles/node_modules:/Applications/XAMPP/node_modules:/Applications/node_modules:/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/regexpu-core@4.8.0/node_modules/regexpu-core/node_modules:/Applications/XAMPP/xamppfiles/htdocs/vite/my-vitesse-app/node_modules/.pnpm/regexpu-core@4.8.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../regjsparser@0.7.0/node_modules/regjsparser/bin/parser" "$@"
else
  exec node  "$basedir/../../../../../regjsparser@0.7.0/node_modules/regjsparser/bin/parser" "$@"
fi
